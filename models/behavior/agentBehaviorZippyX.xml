                                            <Fallback>
                                                <Action ID="ASSIGN_PARK_SPOT" />
                                                <Action ID="MOVE" />
                                            </Fallback>
                                        </SequenceWithMemory>
                                    </Fallback>
                                </Fallback>
                                <Inverter>
                                    <Action ID="CHECK_PARK_LOCATION_ASSIGNED" name="" />
                                </Inverter>
                            </Sequence>
                            <Action ID="MOVE" />
                            <Action ID="DOCK" />
                            <Action ID="CHARGE" />
                            <Action ID="UNDOCK" />
                        </SequenceWithMemory>
                    </Fallback>
                    <SequenceWithMemory name="TaskSeq">
                        <Fallback name="ParkAssignFallback">
                            <Action ID="CHECK_TASK_ASSIGNED" />
                            <Action ID="CHECK_PARK_ASSIGNED" />
                            <Action ID="ASSIGN_PARK_SPOT" />
                        </Fallback>
                        <Action ID="MOVE_PICK_MOVABLE" />
                        <Fallback name="CheckNewTask">
                            <Action ID="CHECK_TASK_ASSIGNED" />
                            <Action ID="ASSIGN_NEW_TASK" />
                        </Fallback>
                        <Action ID="MOVE_PICK_MOVABLE" />
                        <Action ID="DOCK_ZIPPYX" />
                        <Action ID="LOAD_ZIPPYX" />
                        <Action ID="MOVE_PICK_MOVABLE" />
                        <Action ID="DOCK_ZIPPYX" />
                        <Action ID="UNLOAD_ZIPPYX" />
                        <Fallback name="CheckNewTask1">
                            <Action ID="CHECK_TIPPING_STATION" />
                            <Action ID="ASSIGN_NEW_TASK" />
                        </Fallback>

                    </SequenceWithMemory>
                </SequenceWithMemory>
            </Fallback>

        </SequenceWithMemory>
    </BehaviorTree>
